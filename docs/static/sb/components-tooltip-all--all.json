      "source": "() => {\n  const position = 'bottom';\n  const appendToBody = true;\n  const hoverable = false;\n  const tooltip = 'An awesome tooltip';\n\n  const options = {\n    tooltip,\n    hoverable,\n    position,\n    appendToBody,\n  };\n\n  return (\n    <div style={{ marginTop: '30px', marginLeft: '150px' }}>\n      <Tooltip {...options}>\n        <Button>{position}</Button>\n      </Tooltip>\n    </div>\n  );\n}"
    },
