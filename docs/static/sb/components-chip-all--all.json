    "storySource": {
      "source": "() => {\n  const type = 'selection';\n\n  const label = 'Chip Label';\n\n  const icon = 'assessment';\n\n  const disabled = false;\n\n  let clearButton;\n  if (type !== 'action') {\n    clearButton = true;\n  }\n\n  let selected;\n  if (type === 'selection') {\n    selected = false;\n  }\n  return (\n    <Chip\n      icon={icon}\n      label={label}\n      clearButton={clearButton}\n      disabled={disabled}\n      type={type}\n      onClose={action(`onClose: ${name}`)}\n      onClick={action(`onClick: ${name}`)}\n      selected={selected}\n      name={'chip'}\n    />\n  );\n}"
    },
