{
  "$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
  "projectFolder": ".",
  "mainEntryPointFilePath": "<projectFolder>/dist/core/index.type.d.ts",
  "bundledPackages": [],
  "compiler": {
    "tsconfigFilePath": "<projectFolder>/tsconfig.type.json",
    "overrideTsconfig": {
      "compilerOptions": {
        "skipLibCheck": true,
        "types": [],
        "lib": ["es6", "es7", "es2017", "dom"],
        "moduleResolution": "node"
      }
    }
  },
  "apiReport": {
    "enabled": false
  },
  "docModel": {
    "enabled": false
  },
  "dtsRollup": {
    "enabled": true,
    "untrimmedFilePath": "<projectFolder>/dist/types/<unscopedPackageName>.d.ts",
    "publicTrimmedFilePath": "<projectFolder>/dist/index.type.d.ts",
    "omitTrimmingComments": false,
    "betaTrimmedFilePath": ""
  },
  "tsdocMetadata": {
    "enabled": false
  },
  "messages": {
    "compilerMessageReporting": {
      "default": {
        "logLevel": "warning"
      }
    },
    "extractorMessageReporting": {
      "default": {
        "logLevel": "warning",
        "addToApiReportFile": true
      },
      "ae-missing-release-tag": {
        "logLevel": "none"
      },
      "ae-internal-missing-underscore": {
        "logLevel": "none"
      },
      "ae-forgotten-export": {
        "logLevel": "warning",
        "addToApiReportFile": true
      }
    },
    "tsdocMessageReporting": {
      "default": {
        "logLevel": "warning"
      },
      "tsdoc-undefined-tag": {
        "logLevel": "none"
      }
    }
  }
}
