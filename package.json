{
  "name": "@innovaccer/design-system",
  "version": "5.0.0-10",
  "description": "React components library project for Innovaccer Design System",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/core/index.type.d.ts",
  "umd:main": "dist/index.umd.js",
  "directories": {
    "css": "css"
  },
  "scripts": {
    "chromatic": "chromatic",
    "test": "jest --coverage",
    "test:ci": "jest --onlyChanged",
    "storybook": "storybook dev -p 5000",
    "dev": "concurrently --kill-others --raw \"npm run watch-css\" \"npm run storybook\"",
    "build-storybook": "storybook build -o .dist",
    "deploy-storybook": "storybook-to-aws-s3",
    "setup-css-dev": "./scripts/setup-local-css.sh",
    "pre-commit": "lint-staged",
    "lint:types": "tsc -p '.' --noEmit",
    "lint:check": "eslint ./core --ext .tsx,.ts,.js,.jsx",
    "lint": "npm run lint:check -- --fix",
    "prettier:check": "prettier --config ./.prettierrc --check",
    "prettier": "prettier --config ./.prettierrc --write 'css/src/**/*.css'",
    "build-css": "gulp --gulpfile css/gulpfile.js build",
    "clean-css": "gulp --gulpfile css/gulpfile.js clean",
    "watch-css": "gulp --gulpfile css/gulpfile.js build && gulp --gulpfile css/gulpfile.js watch",
    "build-js": "rm -rf dist/* && rollup -c rollup.config.mjs",
    "build-type": "rm -rf .lib && tsc -b tsconfig.type.json",
    "build": "npm run build-css && npm run build-js",
    "version": "./scripts/version.sh",
    "prepare": "husky install",
    "generate-types": "node node_modules/.bin/dts-bundle-generator --config dts.config.js"
  },
  "author": "Innovaccer",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.24.5",
    "@babel/core": "^7.27.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@commitlint/cli": "19.5.0",
    "@commitlint/config-conventional": "^12.1.4",
    "@figma/code-connect": "^1.0.1",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-image": "^3.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@storybook/addon-a11y": "^9.0.4",
    "@storybook/addon-docs": "^9.0.4",
    "@storybook/react-webpack5": "^9.0.4",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^10.4.7",
    "@types/jest": "^29.5.12",
    "@types/js-beautify": "^1.13.3",
    "@types/lz-string": "^1.3.34",
    "@types/node": "^22.15.17",
    "@types/react": "^16.14.56",
    "@types/react-dom": "^16.9.24",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/throttle-debounce": "^2.1.0",
    "@types/webpack": "^5.28.5",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "all-contributors-cli": "^6.20.0",
    "autoprefixer": "^10.4.19",
    "babel-loader": "^8.4.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "bl": ">=1.2.3",
    "brotli": "^1.3.2",
    "chromatic": "^6.5.6",
    "commitlint": "19.5.0",
    "concurrently": "^5.1.0",
    "dts-bundle-generator": "5.9.0",
    "eslint": "^8.57.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-storybook": "^9.0.4",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-concat": "^2.6.1",
    "gulp-postcss": "^9.0.1",
    "gulp-sourcemaps": "^2.6.5",
    "husky": "^7.0.1",
    "identity-obj-proxy": "^3.0.0",
    "immer": ">=9.0.6",
    "install": "^0.13.0",
    "jest": "^29.7.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.2",
    "lz-string": "^1.4.4",
    "node-notifier": ">=8.0.1",
    "postcss": "^8.4.38",
    "postcss-color-mod-function": "^3.0.3",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.2.5",
    "react": "^16.12.0",
    "react-element-to-jsx-string": "^15.0.0",
    "react-is": "^16.12.0",
    "react-live": "^2.2.2",
    "react-syntax-highlighter": "^15.6.1",
    "rollup": "^4.18.0",
    "rollup-plugin-esbuild": "^6.1.1",
    "rollup-plugin-gzip": "^2.5.0",
    "rollup-plugin-node-builtins": "2.0.0",
    "rollup-plugin-postcss": "^4.0.2",
    "storybook": "^9.0.4",
    "trim": ">=0.0.3",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "typescript-eslint-parser": "^22.0.0",
    "typescript-transform-paths": "^1.1.14"
  },
  "overrides": {
    "@storybook/builder-webpack5": {
      "webpack": "^5.0.0"
    },
    "@storybook/react-docgen-typescript-plugin": {
      "react-docgen-typescript": "^2.2.2"
    },
    "storybook": "$storybook"
  },
  "dependencies": {
    "@lottiefiles/react-lottie-player": "^3.5.3",
    "@storybook/storybook-deployer": "^2.8.16",
    "classnames": "^2.3.1",
    "core-js": "^3.9.1",
    "react-dom": "^16.8.6",
    "react-popper": "^1.3.7",
    "throttle-debounce": "^2.1.0",
    "utility-types": "^3.10.0"
  },
  "bugs": "https://github.com/innovaccer/design-system/issues",
  "homepage": "https://mds.innovaccer.com",
  "keywords": [
    "React",
    "Design System",
    "Typescript",
    "Innovaccer"
  ],
  "repository": "github:innovaccer/design-system",
  "lint-staged": {
    "core/**/*.{tx,tsx,js,jsx}": [
      "npm run lint:check"
    ],
    "css/src/**/*.css": [
      "npm run prettier:check"
    ]
  },
  "files": [
    "dist",
    "css",
    "types",
    "LICENSE",
    "README.md",
    "CONTRIBUTING.md",
    "CODE_OF_CONDUCT.md",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">=20.15.0",
    "npm": ">=10.7.0"
  }
}
